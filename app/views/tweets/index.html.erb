<header>
  <ul>
    <li><a href="#">ホーム</a></li>
    <li><a href="#">モーメント</a></li>
    <li><a href="#">通知</a></li>
    <li><a href="#">メッセージ</a></li>
     <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
    <ul class="nav navbar-nav navbar-right">
      <li><%= link_to "Tweet", new_tweet_path, class: 'btn btn-primary' %></li>
  </ul>
    </ul>
    </div>
</header>
<div class="container">
   <div class="wrapper col-md-6 col-md-offset-3 col-sm-10 col-sm-offset-1">
<%= notice %>
<h1>Tweets</h1>

<% @tweets.each do |tweet| %>
<div class="row">
  <p>本文:<%= tweet.content %></p>
  <%= link_to "編集", edit_tweet_path(tweet.id), class: 'btn btn-default btn-sm btn-success'  %>
  <%= link_to '削除', tweet_path(tweet.id), method: :delete,data: { confirm: '本当削除していいですか？' }, class: 'btn btn-default btn-sm btn btn-danger'  %>
</div>
<% end %>
  </div>
</div>