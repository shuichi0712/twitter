<div class="container">
   <div class="wrapper col-md-6 col-md-offset-3 col-sm-10 col-sm-offset-1">
        <h2>Tweetを編集する</h2>
<%= form_for(@tweet) do |f| %>
<%= f.label :content ,"内容"%>
<%= f.text_field :content, class: 'form-control'  %>
<br>
<%= f.submit"tweet", class: 'btn btn-primary btn-block' %>
<%= f.submit "もどる", name: 'back', class: 'btn btn-default btn-block'  %>
<% end %>
</div>
</div>